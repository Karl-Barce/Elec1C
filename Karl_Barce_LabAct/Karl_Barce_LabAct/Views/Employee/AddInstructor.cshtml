@model Instructor
@{
}

<form asp-action="AddInstructor">
    <div>
        <label class="mt-3">First Name</label>
        <input asp-for="FirstName" class="form-control" />
    </div>
    <div>
        <label class="mt-3">Last Name</label>
        <input asp-for="LastName" class="form-control" />
    </div>
    <div>
        <label class="mt-3">isTenured</label>
        <input type="checkbox" asp-for="isTenured" class="form-check" />
    </div>
    <div>
        <label class="mt-3">Position</label>
        <select asp-for="Rank" asp-items="Html.GetEnumSelectList<Rank>()" aria-label="Rank" class="form-control" disabled></select>
    </div>
    <div>
        <label class="mt-3">Hiring Date</label>
        <input asp-for="HiringDate" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>


</form>